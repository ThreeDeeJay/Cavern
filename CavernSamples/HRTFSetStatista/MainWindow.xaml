<Window x:Class="HRTFSetStatista.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HRTFSetStatista"
        mc:Ignorable="d"
        Title="HRTF Set Statista" Height="450" Width="800">
    <Grid>
        <TextBlock HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="File name format ({Y},{X}: angles, {D}: distance (m)):" VerticalAlignment="Top"/>
        <TextBox x:Name="setName" Height="23" Margin="10,31,507,0" TextWrapping="Wrap" Text="SetName_{Y}_{X}_{D}.wav" VerticalAlignment="Top" VerticalContentAlignment="Center"/>
        <Button Content="Load set" HorizontalAlignment="Left" Margin="10,59,0,0" VerticalAlignment="Top" Width="75" Click="LoadSet"/>
    </Grid>
</Window>
